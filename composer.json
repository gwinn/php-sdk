{
    "name": "ecomlogic/php-sdk",
    "description": "PHP client for eComlogic API",
    "type": "library",
    "keywords": ["API", "eComlogic", "REST"],
    "homepage": "http://www.ecomlogic.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "eComlogic",
            "email": "dev@ecomlogic.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "support": {
        "email": "dev@ecomlogic.com"
    },
    "autoload": {
        "psr-0": { "Ecomlogic\\": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 600
    }
}
